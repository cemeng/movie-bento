<h1>Listing movie_categories</h1>

<table>
  <tr>
    <th>Movie</th>
    <th>Category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @movie_categories.each do |movie_category| %>
  <tr>
    <td><%= movie_category.movie_id %></td>
    <td><%= movie_category.category_id %></td>
    <td><%= link_to 'Show', movie_category %></td>
    <td><%= link_to 'Edit', edit_movie_category_path(movie_category) %></td>
    <td><%= link_to 'Destroy', movie_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Movie category', new_movie_category_path %>
